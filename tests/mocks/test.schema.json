{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "requiredStringValue": {
            "type": "string"
        },
        "patternValue": {
            "type": "string",
            "pattern": "^[1-9]\\d*$"
        },
        "enumValue": {
            "type": "string",
            "enum": [
                "A",
                "B"
            ]
        },
        "dateValue": {
            "type": "string",
            "format": "date-time"
        },
        "integerValue": {
            "type": "integer"
        },
        "constValue": {
            "const": "CONST"
        }
    },
    "required": [
        "requiredStringValue"
    ]
}
